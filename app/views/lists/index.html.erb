<div class="lists-list">
  <% @lists.each do |list| %>
    <%= link_to list_tasks_path(list) do %>
      <div class="card">
        <%= list.name %>
        <%= link_to "edit list", edit_list_path(list) %>
        <%= link_to "delete list", list_path(list), method: :delete  %>
      </div>
    <% end %>
  <% end %>
</div>


<%= link_to "Create a new list", new_list_path %>
